        <section name="scriptor">
            <section name="body">
                <value><![CDATA[private com.enterprisehorizons.magma.designtime.artifact.GeoArtifact $artefact;
private com.enterprisehorizons.magma.datamashup.ShapeFileDataDriver $shapefilemashupdriver;

void bindData() {
	
    com.enterprisehorizons.conversion.shp.ShapeFileRecord data = $shapefilemashupdatamodel;
    String[] attributeNames = $shapefilemashupdriver.getMetaData();
    //$artefact.setAttributeNames(attributeNames);
    //$artefact.setAttributeValues($shapefilemashupdatamodel.getAttributeValues());
    $artefact.setCoordinates(data.getCoordinates());
    $artefact.addAttribute("SiteID", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "SiteID", false)));
	$artefact.addAttribute("SensorDate", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "SensorDate", false)));
	$artefact.addAttribute("SensorID", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "SensorID", false)));
	$artefact.addAttribute("Confidence", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "Confidence", false)));
	$artefact.addAttribute("FieldValid", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "FieldValid", false)));
	$artefact.addAttribute("Settlement", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "Settlement", false)));
	$artefact.addAttribute("Notes", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "Notes", false)));
	$artefact.addAttribute("Main_Damag", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "Main_Damag", false)));
	$artefact.addAttribute("Grouped_Da", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "Grouped_Da", false)));
	$artefact.addAttribute("StaffID", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "StaffID", false)));
	$artefact.addAttribute("EventCode", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "EventCode", false)));
	$artefact.addAttribute("ImageID_Nu", data.getAttributeValueObject(ArrayUtils.getMatchedIndex(attributeNames, "ImageID_Nu", false)));


            
}

String getName() {
	return $artefact.getAttributeValueAsString("SiteID");
}]]></value>
